    "def get_neighbours(state):\n",
    "    neighbours = [(state[0] - 1, state[1]),\n",
    "                  (state[0] + 1, state[1]),\n",
    "                  (state[0], state[1] + 1),\n",
    "                  (state[0], state[1] - 1)]\n",
    "    return neighbours"
   ]
  }
